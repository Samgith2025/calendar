{
  "expo": {
    "name": "Trading Tracker",
    "slug": "trading-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trading-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.samson.tradingtracker",
      "appleTeamId": "5KVBK89452",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.samson.tradingtracker"
        ]
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#111827"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.samson.tradingtracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "bundleIdentifier": "com.samson.tradingtracker.widget",
                  "targetName": "TradingTrackerWidget",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.samson.tradingtracker"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "6ba7eaec-53a1-4e69-981e-e1efa8d1ecd9"
      }
    }
  }
}
